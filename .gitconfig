[user]
  name = Shotaro Yamashita
  email = dadayamism@gmail.com
[color]
  ui          = auto
  status      = auto
  diff        = auto
  branch      = auto
  interactive = auto
  grep        = auto
[push]
  default = upstream
[merge]
  ff = false
[core]
  excludesfile = ~/.gitignore_global
  autocrlf     = input
  editor       = vim
[alias]
  st     = status
  sw     = show
  sh     = stash
  shp    = stash pop
  f      = fetch
  p      = push
  po     = push origin
  pu     = push -u origin
  pl     = pull
  a      = add
  aa     = add --all
  c      = commit
  cm     = commit -m
  ca     = commit -a
  em     = commit -a --allow-empty-message -m ''
  ae     = commit --allow-empty -m
  camend = commit --amend -m
  m      = merge
  mn     = merge --no-ff
  co     = checkout
  b      = branch
  srb    = svn rebase
  sdc    = svn dcommit
[init]
  templatedir = ~/.git_template
[difftool "sourcetree"]
	cmd = /Applications/p4merge.app/Contents/MacOS/p4merge \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = " "
	trustExitCode = true
